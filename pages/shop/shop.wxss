@import '../common/common.wxss';
@import '../common/number.wxss';

.goodsPrice{
  width: 250rpx;
  text-align: center;
  margin: 10rpx;
}
.goodsName{
  width: 250rpx;
  text-align: center;
  margin: 10rpx;
}
.goodsImg{
    width:250rpx;
    height:250rpx;
}


.tui-fixed-x{
  margin-left: 260rpx;
  position: fixed;
  top: 0;
  left: 0;
}
.tui-fixed-left{
  width: 240rpx;
  height: 100%;
  border-right: 1px solid #dfdfdf;
  position: fixed;
  background-color: #f5f5f5;
  top: 0;
  left: 0;
}

.tui-city-scroll{
  width: 100%;
}
.tui-nav-li{
  font-size: 30rpx;
  padding: 0 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  width: 200rpx;
  display: block;
  border-bottom: 2rpx solid #dfdfdf;
  position: relative;
  z-index: 10;
}
.tui-nav-active{
  background-color: #fff;
}

.tui-fixed-y{
  width: 500rpx;
  height: 100%;
  position: fixed;
  bottom: 0;
  right: 0;
}
.tui-city-scroll-y{
  height: 100%;
  box-sizing: border-box;
}
.tui-list-head{
  height: 80rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 30rpx;
  color: blue;
}
.tui-list-li{
  width: 100%;
  display: flex;
  flex-direction: column;

}




